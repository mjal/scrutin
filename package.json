{
  "name": "scrutin-app",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export:web": "expo export:web",
    "export:web:_redirects": "echo '/*   /index.html   200' > web-build/_redirects",
    "re:build": "rescript build -with-deps",
    "re:clean": "rescript clean",
    "re:rebuild": "rescript clean && rescript build -with-deps",
    "re:start": "rescript build -with-deps -w",
    "doc": "docco -L assets/languages.json src/*.res src/**/*.res",
    "eas-build-post-install": "rescript clean && rescript build -with-deps",
    "test": "cypress run",
    "test:gui": "cypress open"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.3",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@rescript-react-native/paper": "git+https://github.com/scrutin-app/paper",
    "@rescript-react-native/svg": "^12.1.9",
    "@rescript/react": "^0.10.3",
    "@ryyppy/rescript-promise": "^2.1.0",
    "add": "^2.0.6",
    "email-validator": "^2.0.4",
    "expo": "~47.0.12",
    "expo-status-bar": "~1.4.2",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "^12.2.0",
    "react-native": "^0.70.8",
    "react-native-get-random-values": "^1.8.0",
    "react-native-paper": "^5.1.3",
    "react-native-safe-area-context": "4.4.1",
    "react-native-svg": "13.4.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.9",
    "rescript-json-combinators": "git+https://github.com/scrutin-app/rescript-json-combinators.git",
    "rescript-react-native": "^0.70.0",
    "rescript-react-native-async-storage": "git+https://github.com/scrutin-app/rescript-react-native-async-storage",
    "rescript-use-tea": "git+https://github.com/scrutin-app/rescript-use-tea.git",
    "rescript-webapi": "git+https://github.com/scrutin-app/rescript-webapi.git",
    "sjcl-with-all": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "cypress": "^12.9.0",
    "docco": "^0.9.1",
    "rescript": "^10.1.2"
  },
  "private": true
}

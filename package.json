{
  "name": "scrutin-app",
  "version": "0.0.3",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "export:web": "expo export:web",
    "export:web:_redirects": "echo '/*   /index.html   200' > web-build/_redirects",
    "re:build": "rescript build -with-deps",
    "re:clean": "rescript clean",
    "re:rebuild": "rescript clean && rescript build -with-deps",
    "re:start": "rescript build -with-deps -w",
    "doc": "docco -L assets/languages.json src/*.res src/**/*.res",
    "eas-build-post-install": "npx rescript -v ; rescript clean && rescript build -with-deps",
    "test": "cypress run",
    "test:gui": "cypress open",
    "bbs": "cd bbs && npm run start:dev",
    "auth": "cd auth && npm run start:dev",
    "build:android": "eas build --platform android --local --profile development"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/webpack-config": "^18.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@rescript-react-native/paper": "https://github.com/scrutin-app/rescript-react-native-paper#1eb4e84",
    "@rescript-react-native/svg": "^12.1.9",
    "@rescript/react": "^0.10.3",
    "@ryyppy/rescript-promise": "^2.1.0",
    "@types/react": "~18.0.27",
    "expo": "~48.0.11",
    "expo-crypto": "~12.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-splash-screen": "^0.18.1",
    "expo-status-bar": "~1.4.4",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "intl-pluralrules": "^2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.71.6",
    "react-native-get-random-values": "^1.8.0",
    "react-native-paper": "^5.7.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.10",
    "rescript-react-native": "^0.70.0",
    "rescript-react-native-async-storage": "https://github.com/scrutin-app/rescript-react-native-async-storage",
    "rescript-webapi": "^0.7.0",
    "sirona": "^0.0.5",
    "sjcl-with-all": "^1.0.8",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@badeball/cypress-cucumber-preprocessor": "^16.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^12.9.0",
    "docco": "^0.9.1",
    "rescript": "^10.1.4"
  },
  "browser": {
    "crypto": false,
    "stream": false
  },
  "private": true
}
